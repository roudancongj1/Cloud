<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sch.dao.UserMapper">
    <update id="updateFdNum" parameterType="map">
        update user set feedback_num=#{feedbackNum} where user_number=#{userNumber}
    </update>
    <update id="resetFdNum" parameterType="int">
        update user set feedback_num = #{num}
    </update>
</mapper>
